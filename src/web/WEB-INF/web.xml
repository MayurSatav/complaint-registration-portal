<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>dataconn</servlet-name>
        <servlet-class>dataconn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertSignup</servlet-name>
        <servlet-class>InsertSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertPost</servlet-name>
        <servlet-class>InsertPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckLogin</servlet-name>
        <servlet-class>CheckLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dataconn</servlet-name>
        <url-pattern>/dataconn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertSignup</servlet-name>
        <url-pattern>/InsertSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertPost</servlet-name>
        <url-pattern>/InsertPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckLogin</servlet-name>
        <url-pattern>/CheckLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
